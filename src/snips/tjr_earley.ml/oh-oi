            ~nt:(fun _Y -> 
                (* have we already processed k Y? *)
                let bitms = bitms (k,_Y) in
                let bitms_empty = nt_item_set_ops.is_empty bitms in
                (* NOTE already_processed_kY = not bitms_empty *)
                let s0 = add_bitm_at_k bitm _Y s0 in
                assert(log P.fg);
                bitms_empty |> function