                | false -> (
                    (* already processed k Y, so no need to expand; but
                       may have complete item kYk *)
                    debug_endline "not bitms_empty";
                    mem_ixk_done (k,_Y) s0 |> function
                    | true -> add_todo (cut bitm k) s0
                    | false -> s0)  (* FIXME waypoint? *)